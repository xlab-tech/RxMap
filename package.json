{
  "name": "@rxmap/rxmap",
  "version": "0.1.0",
  "description": "map command and reactive ",
  "keywords": [
    "map",
    "reactive",
    "command",
    "observer",
    "leaflet",
    "googlemaps",
    "geo"
  ],
  "main": "dist/umd/RxMap.min.js",
  "module": "dist/esm/index.js",
  "files": [
    "lib.js",
    "dist/**",
    "src/**",
    "lib/**",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "cross-env BABEL_ENV=test mocha -c \"test/unit/**/*.spec.js\" --require @babel/register -r jsdom-global/register -r @babel/polyfill",
    "test:cover": "cross-env BABEL_ENV=test ./node_modules/.bin/istanbul cover -x \"lib/**\" ./node_modules/mocha/bin/_mocha -- \"test/unit/**/*.spec.js\"  --recursive --require @babel/register -r jsdom-global/register -r @babel/polyfill",
    "test:tdd": "cross-env BABEL_ENV=test mocha -c \"test/unit/**/*.spec.js\" --require @babel/register -r jsdom-global/register -r @babel/polyfill --watch --watch-extensions js",
    "test:debug": "cross-env BABEL_ENV=test mocha -c \"test/unit/**/*.spec.js\" --require @babel/register -r jsdom-global/register -r @babel/polyfill --inspect-brk ",
    "lint": "eslint \"./**/*.js\" --fix",
    "doc": "./node_modules/.bin/esdoc",
    "start": "webpack-dev-server --open",
    "build": "webpack --config webpack.pro.config.js && babel src -d dist/esm --ignore './src/index.umd.js'",
    "prepare": "npm run doc && npm run build",
    "build:es": "babel lib -d dist/lib2015"
  },
  "author": "Victor Rubinat",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/xlab-tech/RxMap.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-stage-1": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@rxmap/basiclib": "^1.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "dotenv-webpack": "^1.5.7",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-importpath-plugin": "^1.0.2",
    "esdoc-integrate-test-plugin": "^1.0.0",
    "esdoc-lint-plugin": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-import": "^2.14.0",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-jsdom": "^2.0.0",
    "sinon": "^6.3.3",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "core-js": "^2.5.7",
    "google-maps": "^3.3.0",
    "leaflet": "^1.3.4",
    "mapbox-gl": "^0.49.0",
    "ol": "^5.2.0",
    "proxy-polyfill": "^0.3.0",
    "rxjs": "^6.3.2",
    "url-polyfill": "^1.1.0"
  }
}
