{
  "name": "@rxmap/rxmap",
  "version": "0.0.2",
  "description": "map command and reactive ",
  "keywords": [
    "map",
    "reactive",
    "command",
    "observer",
    "leaflet",
    "googlemaps",
    "geo"
  ],
  "main": "dist/umd/RxMap.min.js",
  "module": "dist/esm/index.js",
  "files": [
    "lib.js",
    "dist/**",
    "src/**",
    "lib/**",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "cross-env BABEL_ENV=test mocha -c \"test/unit/**/*.spec.js\" --require babel-register -r jsdom-global/register",
    "test:cover": "cross-env BABEL_ENV=test ./node_modules/.bin/istanbul cover -x \"lib/**\" ./node_modules/mocha/bin/_mocha -- \"test/unit/**/*.spec.js\"  --recursive --require babel-register -r jsdom-global/register",
    "test:tdd": "cross-env BABEL_ENV=test mocha -c \"test/unit/**/*.spec.js\" --require babel-register -r jsdom-global/register --watch --watch-extensions js",
    "test:debug": "cross-env BABEL_ENV=test mocha -c \"test/unit/**/*.spec.js\" --require babel-register -r jsdom-global/register --inspect-brk ",
    "lint": "eslint \"./**/*.js\" --fix",
    "doc": "./node_modules/.bin/esdoc",
    "start": "webpack-dev-server --open",
    "build": "webpack --config webpack.pro.config.js && babel src -d dist/esm",
    "prepare": "npm run doc && npm run build",
    "build:es": "babel lib -d dist/lib2015"
  },
  "author": "Victor Rubinat",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/xlab-tech/RxMap.git"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.3",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-importpath-plugin": "^1.0.2",
    "esdoc-integrate-test-plugin": "^1.0.0",
    "esdoc-lint-plugin": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-jsdom": "^2.0.0",
    "sinon": "^6.1.5",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.6"
  },
  "dependencies": {
    "core-js": "^2.5.7",
    "google-maps": "^3.3.0",
    "leaflet": "^1.3.4",
    "proxy-polyfill": "^0.3.0",
    "rxjs": "^6.2.2",
    "url-polyfill": "^1.0.14"
  }
}
